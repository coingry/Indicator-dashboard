{
  "name": "indicator-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",

    "socket": "tsx server/socket-server.ts",
    "dev:all": "concurrently -k -n web,socket -c blue,magenta \"npm:dev\" \"npm:socket\"",
    "build:socket": "esbuild server/socket-server.ts --bundle --platform=node --target=node20 --format=cjs --outfile=dist/socket-server.js",
    "build:all": "npm run build && npm run build:socket",
    "start:all": "concurrently -k -n web,socket -c blue,magenta \"npm:start\" \"node dist/socket-server.js\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/query-sync-storage-persister": "^5.85.5",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-persist-client": "^5.85.5",
    "date-fns": "^4.1.0",
    "http-proxy": "^1.18.1",
    "lightweight-charts": "^4.2.3",
    "next": "15.5.0",
    "node-fetch": "^3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.9",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
